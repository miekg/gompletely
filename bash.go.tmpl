# {{.Command}} completion

# Generated by gompletely (https://github.com/miekg/gompletely)

_{{.Command}}_completions_filter() {
  COMP_REPLY=()
  local words="$1"
  local cur=${COMP_WORDS[COMP_CWORD]}
  local result=()

  if [[ "${cur:0:1}" == "-" ]]; then
    echo "$words"

  else
    for word in $words; do
      [[ "${word:0:1}" != "-" ]] && result+=("$word")
    done

    echo "${result[*]}"

  fi
}

_{{.Command}}_completions() {
  local cur=${COMP_WORDS[COMP_CWORD]}
  local compwords=("${COMP_WORDS[@]:1:$COMP_CWORD-1}")
  local compline="${compwords[*]}"

  case "$compline" in
{{range .Patterns}}
  {{.CaseString}})
    {{if .Positional}}{{.Positional}}{{end}}
	while read -r; do COMPREPLY+=("$REPLY"); done < <(compgen {{.CompGen}} -- "$cur")
      ;;
  {{end}}
  esac
} &&
complete -F _{{.Command}}_completions {{.Command -}}
